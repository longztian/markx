{
  "name": "markless",
  "version": "0.1.0",
  "description": "yet another markup language, inspired by markdown",
  "main": "dist/markless.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint src bin",
    "build": "rm -rf build && mkdir -p build/test && node bin/build.js",
    "test": "mocha build/test",
    "dist": "mkdir -p dist && cp build/markless.js dist && node_modules/.bin/uglifyjs dist/markless.js -m -c -p relative --source-map dist/markless.min.js.map -o dist/markless.min.js",
    "cbtest": "node bin/browser_test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/longztian/markless.git"
  },
  "keywords": [
    "markless",
    "markdown",
    "markup"
  ],
  "author": "Longzhang Tian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/longztian/markless/issues"
  },
  "homepage": "https://github.com/longztian/markless#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "finalhandler": "^0.5.0",
    "mocha": "^3.0.2",
    "readdirp": "^2.1.0",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "serve-static": "^1.11.1",
    "uglify-js": "^2.7.3"
  }
}
